doctype html
html
    head
        title Product Demo Request submitted from the Alcumus Portal
        style
            include send-request-style.css
    body 
        table.top-table(width='100%', height='100%', cellpadding='0', cellspacing='0', border='0', align='left', valign='top')
            tbody
                tr
                    td.img-col(align='center', valign='top')
                        table.inner-table(width='600', align='left', cellpadding='0', cellspacing='0', border='0', valign='top')
                            tbody
                                tr
                                    td.inner-table-row-column(align='left')
                                        span.innermost-table-column-span
                                            h1
                                                a.anchor(href='https://alcumus.com', target='_blank')
                                                    img(src='data:image/png;base64,', alt='Alcumus Logo')
                        table.innermost-table(align='left', cellpadding='0', cellspacing='0', border='0', valign='top')
                            tbody
                                tr
                                td.innermost-table-column
                                    p Hello there,
                                    p A product demo request was submitted on the Alcumus Portal. Please see the details below.
                                    br
                                    b Details:
                                    br
                                    br
                                    p.field-title Date Submitted
                                    p.para-margin #{requestDate}
                                    p.field-title Customer Name
                                    p.para-margin #{tenantName}
                                    p.field-title Product
                                    p.para-margin #{productName}
                                    p.field-title User Name
                                    p.para-margin #{sendersName}
                                    p.field-title User Email
                                    p.para-margin #{useremail}
                                    br
                                    p Thank you.
