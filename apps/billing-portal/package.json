{
  "name": "billing-portal",
  "version": "2.13.0",
  "description": "A starter app for frontend projects with universal react, material-ui and redux support",
  "main": "build/server/index.js",
  "private": true,
  "scripts": {
    "build": "tsc && webpack",
    "clean": "rimraf build ",
    "start": "cross-env NODE_ENV=production node ./build/server/index.js",
    "start-dev": "nodemon",
    "start:watch": "npm run clean && npm run build && nodemon",
    "lint:ts": "eslint src/**/*.ts && eslint src/**/*.tsx",
    "lint:css": "stylelint \"./src/**/*.tsx\"",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "eslint --fix .",
    "prettier:verify": "prettier --check .",
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "test:changed": "jest --runInBand --onlyChanged",
    "storybook": "start-storybook -p 6004 -s ./static",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@alcumus/analytics-package": "^2.7.0",
