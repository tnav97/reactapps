    "@mui/material": "5.12.1",
    "@mui/styles": "5.12.0",
    "axios": "^1.3.6",
    "bluebird": "^3.7.2",
    "clsx": "^1.2.1",
    "core-js": "^3.30.1",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "faker": "^6.6.6",
    "fast-deep-equal": "^3.1.3",
    "helmet": "^6.1.5",
    "i18next": "^22.4.15",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-i18next": "^12.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "redis": "^4.6.5",
    "redux": "^4.2.1",
