    "@mui/material": "5.8.5",
    "@mui/styles": "5.8.4",
    "axios": "^0.24.0",
    "bluebird": "^3.7.2",
    "clsx": "^1.1.1",
    "core-js": "^3.21.1",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "faker": "^5.5.3",
    "fast-deep-equal": "^3.1.3",
    "helmet": "^6.0.0",
    "i18next": "^22.4.9",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.2.3",
    "react-i18next": "^12.1.4",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "redis": "^3.1.2",
