    "react-helmet-async": "^1.2.3",
    "react-i18next": "^12.1.4",
    "react-idle-timer": "^4.6.4",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.3.0",
    "react-transition-group": "^4.4.2",
