    "react-helmet-async": "^1.3.0",
    "react-i18next": "^12.2.0",
    "react-idle-timer": "^5.6.2",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.10.0",
    "react-transition-group": "^4.4.5",
