    "@types/mixpanel": "^2.14.4",
    "@types/mixpanel-browser": "^2.38.1",
    "axios": "^1.3.6",
    "bluebird": "^3.7.2",
    "clsx": "^1.2.1",
    "core-js": "^3.30.1",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
