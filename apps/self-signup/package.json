    "@types/mixpanel": "^2.14.3",
    "@types/mixpanel-browser": "^2.38.0",
    "axios": "^0.24.0",
    "bluebird": "^3.7.2",
    "clsx": "^1.1.1",
    "core-js": "^3.21.1",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
