{
  "name": "self-signup",
  "version": "2.13.0",
  "description": "React app to allow customers to self signup into our end products like eCompliance",
  "main": "build/server/index.js",
  "private": true,
  "scripts": {
    "build": "tsc && webpack",
    "clean": "rimraf build ",
    "start": "cross-env NODE_ENV=production node ./build/server/index.js",
    "start-dev": "nodemon",
    "start:watch": "npm run clean && npm run build && nodemon",
    "lint:ts": "eslint src/**/*.ts && eslint src/**/*.tsx",
    "lint:css": "stylelint \"./src/**/*.tsx\"",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "eslint --fix .",
    "prettier:verify": "prettier --check .",
    "test": "jest --runInBand",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --runInBand --watch",
    "test:changed": "jest --runInBand --onlyChanged",
    "storybook": "start-storybook -p 6003 -s ./static",
    "build-storybook": "build-storybook",
    "cypress": "cypress open",
    "cypress:test": "cypress run --browser chrome --headless --spec 'cypress/integration/**/*.spec.ts' --config video=false,retries=1,defaultCommandTimeout=120000,pageLoadTimeout=120000"
  },
