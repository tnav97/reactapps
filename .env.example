FEATURE_TOGGLE_CREATE_SERVICES_ORGANIZATION_ON_SIGNUP=true
FEATURE_TOGGLE_DISABLE_DIRECT_PORTAL_LOGIN=false
FEATURE_TOGGLE_SELF_SIGNUP_SHOW_UPGRADE_CTA=true
FEATURE_TOGGLE_USE_AZURE_AD=true

# When this is set to false, Split.io SDK will run in localhost mode, which is ideal for local, dev and QA environments.
# When set to true, Split.IO SDK will contact it's servers (this is for prod only)
