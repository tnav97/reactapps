{
  "name": "@alcumus/components",
  "version": "2.13.0",
  "description": "A library of React components for Alcumus frontend projects",
  "keywords": [
    "alcumus",
    "react",
    "components"
  ],
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/esm/index.d.ts",
  "private": true,
  "scripts": {
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir build/cjs",
    "build": "npm run build:esm && npm run build:cjs",
    "clean": "rimraf build ",
    "lint:ts": "eslint src/**/*.ts && eslint src/**/*.tsx",
    "lint:css": "stylelint \"./src/**/*.tsx\"",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "eslint --fix .",
    "prettier:verify": "prettier --check .",
    "test": "jest --runInBand",
    "test:changed": "jest --runInBand --onlyChanged",
    "storybook": "start-storybook -p 6789",
    "storybook:build": "rimraf .storybook/build && npm run build-storybook -- -o .storybook/build",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@loadable/component": "^5.15.2",
    "@storybook/addon-a11y": "^6.5.15",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^17.0.45",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18",
    "clsx": "^1.1.1",
    "eslint-plugin-react": "^7.31.11",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "react-router-dom": "^5.3.4",
    "react-test-renderer": "^18.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^27.1.3",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "@loadable/component": "^5.15.2",
    "@mui/icons-material": "5.8.4",
    "@mui/material": "5.8.5",
    "clsx": "^1.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.6.2"
  },
  "dependencies": {
    "@alcumus/hooks": "^2.7.0",
    "@mui/styles": "5.8.4",
    "store": "^2.0.12",
    "use-debounce": "^7.0.1"
  }
}
